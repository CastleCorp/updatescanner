<!DOCTYPE overlay SYSTEM "chrome://updatescan/locale/updatescan.dtd">

<overlay id="USc_ContextOverlay"
         xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul">

  <commandset id="placesCommands">
    <command id="placesCmd_open"
              oncommand="USc_updatescan.diffSelectedItemThisWindow();"/>
    <command id="placesCmd_open_all_in_tabs"
              oncommand="USc_updatescan.diffSelectedItemThisWindow();"/>
    <command id="placesCmd_open:tab"
             oncommand="USc_updatescan.diffSelectedItemNewTab();"/>
    <command id="placesCmd_new:bookmark"
             oncommand="USc_updatescan.openNewDialogCurrentPos();"/>
    <command id="placesCmd_show:info" 
             oncommand="USc_updatescan.openEditDialog();"/>
  </commandset>

